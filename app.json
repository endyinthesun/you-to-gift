{
  "expo": {
    "name": "you-to-gift",
    "slug": "you-to-gift",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/expo/icon.png",
    "splash": {
      "image": "./assets/images/expo/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/expo/adaptive-icon.png",
        "backgroundColor": "#B588F7"
      }
    },
    "web": {
      "favicon": "./assets/images/expo/favicon.png"
    }
  }
}
